d (max-width: 1100px){\n    .Services_Intro {\n        margin-top: 60px;\n    }\n    .Services_Intro-Wrap {\n        display: block;\n    }\n    .Services_Intro-Header {\n        width: 100%;\n        padding: 0;\n    }\n    .Services_Intro-Right {\n        margin-top: 40px;\n        width: 100%;\n        padding: 0;\n    }\n}\n@media screen and (max-width: 900px){\n    .Services_Intro-Header {\n        font-size: 50px;\n    }\n}\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n//Services Section ------- ----------- -----------------\n.Services_Section {\n    width: 100%;\n    margin-top: 200px;\n}\n\n    .Services_Section-Wrap {\n        width: 100%;\n        position: relative;\n    }\n\n        .Services_Section-Service-Container {\n            width: 100%;\n            padding: 80px 0px;\n            border-top: 2px solid #c4c4c4;\n            opacity: 0;\n            transition: all .6s ease-in;\n        }\n            .Services_Section-Service-Container:last-child {\n                border-bottom: 2px solid #c4c4c4;\n            }\n\n            .Services_Section-Service-Label {\n                width: 100%;\n                display: flex;\n                justify-content: space-between;\n                align-items: center;\n                margin-bottom: 40px;\n            }   \n\n                .Services_Section-Service-Label-Content {\n                    display: flex;\n                    justify-content: space-between;\n                    align-items: center;\n                }\n\n                    .Services_Section-Service-Label-Image {\n                        width: 30px;\n                        margin-right: 30px;\n                    }\n\n                    .Services_Section-Service-Label-Text {\n                        font-size: 28px;\n                        font-weight: 500;\n                    }\n\n                .Services_Section-Service-Label-Icon-Box {\n                    width: 75px;\n                    height: 75px;\n                    background-color: #FFF;\n                    box-shadow: 5px 5px 20px rgba(82, 82, 82, 0.05);\n                    display: flex;\n                    justify-content: center;\n                    align-items: center;\n                    transition: all .4s ease-in;\n                    cursor: pointer;\n                    display: none;\n                    \n                }\n\n                    .Services_Section-Service-Label-Icon-Wrap {\n                        width: 25px;\n                        height: 25px;\n                        position: relative;\n                        display: flex;\n                        justify-content: center;\n                        align-items: center;\n                    }\n\n                        .Services_Section-Service-Label-Icon-Bar {\n                            width: 100%;\n                            height: 3px;\n                            background-color: #0700ff;\n                            position: absolute;\n                            transition: all .4s ease-in-out;\n                        }\n\n                            .Service-Label-Icon-Bar-Active {\n                                transform: rotate(90deg);\n                            }\n\n            .Services_Section-Service-Description {\n                width: 100%;\n                height: auto;\n                display: flex;\n                justify-content: center;\n                transition: height .4s ease-in;\n            }\n\n                .Services_Section-Service-Description-Text {\n                    padding: 0 5%;\n                    font-size: 18px;\n                    line-height: 46px;\n                    color: rgb(137, 137, 137);\n                    opacity: 0;\n                    transition: all .4s ease-in;\n                }\n\n                .Active-Text {\n                    opacity: 1;\n                }\n\n@media screen and (max-width: 900px){\n    .Services_Section {\n        margin-top: 130px;\n    }\n    .Services_Section-Service-Description-Text {\n        font-size: 16px;\n        line-height: 28px;\n    }\n    .Services_Section-Service-Container {\n        padding: 60px 0px;\n    }\n}\n\n@media screen and (max-width: 600px){\n    .Services_Section-Service-Label-Text {\n        font-size: 20px;\n    }\n}\n\n\n\n"},"lineCount":null}},"error":null,"hash":"7c8e7ee1bc6935c136f83af524927912","cacheData":{"env":{}}}